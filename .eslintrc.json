{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 2016
    },
    "rules": {
        "indent": [
            "error",
            4
        ],
        "linebreak-style": [
            "error",
            "windows"
        ],
        "quotes": [
            "error",
            "single"
        ],
        "semi": [
            "error",
            "always"
        ],
        "no-console": [
            "error", {
                "allow": ["info"]
            }
        ]
    },
    "globals": {
        "Plotly": "readable",
        "duomenysRackausko": "readable",
        "math": "readable",
        "laboratorinis2Uzd1Data": "readable",
        "laboratorinis2Uzd2Data": "readable",
        "laboratorinis2Uzd3Data": "readable",
        "skaitiniaiMetodai": "readable"
    }
}